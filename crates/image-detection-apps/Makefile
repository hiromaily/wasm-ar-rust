.PHONY: build
build:
	cargo build

.PHONY: run
run:
	cargo run --bin image-detection-apps

.PHONY: run-phash
run-phash:
	cargo run --bin imadetect-phash

.PHONY: run-rect-phash
run-rect-phash:
	cargo run --bin imadetect-rect-phash

.PHONY: run-template-matching
run-template-matching:
	cargo run --bin imadetect-template-matching

.PHONY: run-template-matching2
run-template-matching2:
	cargo run --bin imadetect-template-matching2
